pragma solidity >=0.8.13;

import {IHevm} from "properties/util/Hevm.sol";

contract VyperDeployer {
    IHevm internal constant hevm =
        IHevm(0x7109709ECfa91a80626fF3989D68f67F5b1DD12D);
    
    function deployVotingEscrow(bytes memory args) internal returns (address) {
        bytes memory _bytecode = hex"";

        bytes memory bytecode = abi.encodePacked(_bytecode, args);
        uint256 size = bytecode.length;

        address deployedAddress;
        assembly {
            deployedAddress := create(0, add(bytecode, 0x20), size)
        }

        require(deployedAddress != address(0), "VyperDeployer could not deploy contract");

        return deployedAddress;
    }
}
